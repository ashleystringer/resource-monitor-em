<div>
    <div style="margin-left: 15px;">
        <BsTab as |tab|>
            <tab.pane @title="Test 1">Testing 1 </tab.pane>
            <tab.pane @title="Test 2">
                <div>
                    <FileChart @data={{@model.calls}}/>
                </div>
            </tab.pane>
            <tab.pane @title="Test 3">
                <div>
                    Testing 3
                </div>
            </tab.pane>
            {{#each @model.files as |file|}}
                <tab.pane @title={{file.realname}}>
                    <div>{{file}}</div>
                    <div><File @data={{model}}/></div>
                </tab.pane>
            {{/each}}
        </BsTab>
    </div>
</div>